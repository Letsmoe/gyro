{
	"name": "@gyro-lang/cli",
	"version": "0.0.4",
	"type": "module",
	"description": "The command line interface for Gyro",
	"main": "./lib/index.js",
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"build": "webpack --config ./webpack.config.cjs",
		"watch-build": "webpack --config ./webpack.config.cjs --watch",
		"test-init": "rm -rf ./test/my-app && cd test && node ../lib/index.js init my-app && cd my-app"
	},
	"browser": {
		"fs": false,
		"path": false
	},
	"keywords": [
		"cli",
		"gyro"
	],
	"author": "letsmoe",
	"license": "MIT",
	"dependencies": {
		"axios": "^0.26.1",
		"colarg": "^1.1.2",
		"decompress-zip": "^0.3.3",
		"mime-types": "^2.1.35",
		"prompts": "^2.4.2",
		"qs": "^6.10.3",
		"request": "^2.88.2"
	},
	"bin": {
		"gyst": "./lib/index.js"
	},
	"devDependencies": {
		"@types/node": "^17.0.25"
	}
}
